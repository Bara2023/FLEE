<div class="container">
  <h1>Where do you wanna go?</h1>
  <%= simple_form_for @search do |f| %>
    <%= f.input :airport_start %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :budget_max %>
    <%= f.input :destination %>
    <%= f.input :mood, collection: Travel::MOODS %>
    <%= f.input :age %>
    <%= f.submit "Search" %>
  <% end %>
</div>

<%= render "shared/navbar_search" %>
